<div id="<%= dom_id product %>">
  <p>
    <strong>Category:</strong>
    <%= product.category_id %>
  </p>

  <p>
    <strong>Brand:</strong>
    <%= product.brand_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= product.title %>
  </p>

  <p>
    <strong>Bytitle:</strong>
    <%= product.bytitle %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= product.content %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Old price:</strong>
    <%= product.old_price %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= product.status %>
  </p>

  <p>
    <strong>Keywords:</strong>
    <%= product.keywords %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Img:</strong>
    <%= product.img %>
  </p>

  <p>
    <strong>Hit:</strong>
    <%= product.hit %>
  </p>

</div>
